{
  "$schema": "https://raw.githubusercontent.com/typhonjs-fvtt-lib/validate-manifest/main/schema/strict/module+.json",
  "name": "mindflayer-token-controller",
  "version": "{{version}}",
  "title": "Mind Flayer - Token Controller",
  "description": "Adds functionality to control tokens through external applications or devices. Mind Flayer server (https://github.com/mindflayer-vtt/mindflayer-server) is required for the module to communicate with external services.",
  "minimumCoreVersion": "0.8.0",
  "compatibleCoreVersion": "9.249",
  "author": "749, shawly",
  "authors": [
    {
      "name": "shawly",
      "email": "shawlyde@gmail.com",
      "url": "https://github.com/shawly"
    },
    {
      "name": "749",
      "url": "https://github.com/749"
    }
  ],
  "esmodules": ["./MindFlayer.js"],
  "scripts": [],
  "socket": true,
  "styles": ["./style.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "./lang/en.json"
    }
  ],
  "dependencies": [
    {
      "name": "lib-wrapper",
      "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
    },
    {
      "name": "socketlib",
      "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json"
    }
  ],
  "packs": [
    {
      "name": "mindflayer-token-controller-macros",
      "label": "Mind Flayer: Macros",
      "path": "./packs/macros.db",
      "type": "Macro",
      "entity": "Macro"
    }
  ],
  "media": [],
  "url": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer",
  "changelog": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/releases/",
  "manifest": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/releases/latest/download/module.json",
  "download": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/releases/download/v{{version}}/module.zip",
  "bugs": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/issues",
  "license": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/blob/main/LICENSE",
  "readme": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/blob/main/README.md"
}
