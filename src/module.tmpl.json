  "download": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/releases/download/v{{version}}/module.zip",
  "bugs": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/issues",
  "license": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/blob/main/LICENSE",
  "readme": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer/blob/main/README.md"
