{
  "id": "mindflayer-token-controller",
  "name": "mindflayer-token-controller",
  "version": "{{version}}",
  "title": "Mind Flayer - Token Controller",
  "description": "Adds functionality to control tokens through external applications or devices. Mind Flayer server (https://github.com/mindflayer-vtt/mindflayer-server) is required for the module to communicate with external services.",
  "compatibility": {
    "minimum": "12.0",
    "verified": "12.331"
  },
  "author": "749, shawly",
  "authors": [
    {
      "name": "shawly",
      "email": "shawlyde@gmail.com",
      "url": "https://github.com/shawly"
    },
    {
      "name": "749",
      "url": "https://github.com/749"
    }
  ],
  "esmodules": ["./MindFlayer.js"],
  "scripts": [],
  "socket": true,
  "styles": ["./style.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "./lang/en.json"
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "./lang/de.json"
    },
    {
      "lang": "es",
      "name": "Espa√±ol",
      "path": "./lang/es.json"
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/download/v1.12.15.0/module.json",
        "compatibility": {
          "verified": "1.12.15.0"
        }
      },
      {
        "id": "socketlib",
        "type": "module",
        "manifest": "https://github.com/manuelVo/foundryvtt-socketlib/raw/develop/module.json",
        "compatibility": {
          "verified": "1.1.0"
        }
      }
    ]
  },
  "packs": [
    {
      "name": "mindflayer-token-controller-macros",
      "label": "Mind Flayer: Macros",
      "path": "./packs/macros.db",
      "type": "Macro",
      "entity": "Macro",
      "module": "mindflayer-token-controller"
    }
  ],
  "media": [
    {
      "type": "setup",
      "url": "modules/mindflayer-token-controller/assets/images/mindflayer.png"
    }
  ],
  "url": "https://github.com/mindflayer-vtt/foundryvtt-mindflayer",
